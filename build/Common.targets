<?xml version="1.0" encoding="utf-8"?>
<Project>
  <PropertyGroup>
    <!-- Default Version numbers -->
    <SignAssembly Condition="$(Configuration) == 'Release'">true</SignAssembly>
    <AssemblyOriginatorKeyFile>..\..\build\keys\keypair.snk</AssemblyOriginatorKeyFile>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <NoWarn>1591,1572,1571,1573,1587,1570</NoWarn>
    <Prefer32Bit>false</Prefer32Bit>
    <DebugType>portable</DebugType>
    <ReposRawUri>https://raw.githubusercontent.com/Mpdreamz/elasticsearch-net-abstraction</ReposRawUri>
    <ReposUri>https://github.com/Mpdreamz/elasticsearch-net-abstraction</ReposUri>
    <ReposVersion>master</ReposVersion>
    
    <Copyright>2014-$([System.DateTime]::Now.Year) Elasticsearch BV</Copyright>
    <RepositoryUrl>$(ReposUri)</RepositoryUrl>
    <PackageProjectUrl>$(ReposUri)/blob/$(ReposVersion)/readme.md</PackageProjectUrl>
    <PackageIconUrl>$(ReposRawUri)/$(ReposVersion)/build/nuget-icon.png</PackageIconUrl>
    <PackageReleaseNotes>$(ReposUri)/releases/tag/$(ReposVersion)</PackageReleaseNotes>
    <IncludeSymbols>False</IncludeSymbols>
    
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Condition="'$(TargetFramework)' == 'net461'" Include="Microsoft.NETFramework.ReferenceAssemblies" PrivateAssets="All" Version="1.0.0-preview.2" />
  </ItemGroup>
</Project>
